<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import java.net.URL?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="mkl_shop.manager.placowki.FXMLPlacowkiController">
    <stylesheets>
        <URL value="@fxmlplacowki.css" />
    </stylesheets>
   <children>
      <TableView fx:id="Tabela_Placowki" layoutX="8.0" layoutY="43.0" prefHeight="466.0" prefWidth="1261.0">
        <columns>
          <TableColumn fx:id="Kolumna_Lp" prefWidth="75.0" text="Lp." />
          <TableColumn fx:id="Kolumna_kod_pocztowy" prefWidth="183.0" text="Kod pocztowy" />
            <TableColumn fx:id="Kolumna_Miejscowosc" prefWidth="404.0" text="Miejscowość" />
            <TableColumn fx:id="Kolumna_Adres" prefWidth="336.0" text="Adres" />
            <TableColumn fx:id="Kolumna_Telefon" prefWidth="260.0" text="Telefon" />
        </columns>
         <contextMenu>
            <ContextMenu>
              <items>
                <MenuItem fx:id="Menu_Szczegoly" mnemonicParsing="false" onAction="#goSzczegoly" text="Szczegóły" />
              </items>
            </ContextMenu>
         </contextMenu>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <JFXTextField fx:id="TextField_Placowki" focusColor="#ffff8d" labelFloat="true" layoutX="28.0" layoutY="548.0" prefHeight="31.0" prefWidth="407.0" promptText="Szukaj palcówk po adresie" unFocusColor="#ffff8d" AnchorPane.leftAnchor="28.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </JFXTextField>
      <JFXButton fx:id="Button_Wyjscie" layoutX="1136.0" layoutY="656.0" onAction="#wyjscie" prefHeight="50.0" prefWidth="130.0" text="Wyjście">
         <graphic>
            <FontAwesomeIconView glyphName="CLOSE" size="35" />
         </graphic>
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
